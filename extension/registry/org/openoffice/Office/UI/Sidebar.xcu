<?xml version="1.0" encoding="UTF-8"?>
<oor:component-data
    oor:name="Sidebar"
    oor:package="org.openoffice.Office.UI"
    xmlns:oor="http://openoffice.org/2001/registry"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:install="http://openoffice.org/2004/installation">
    <node oor:name="Content">
        <node oor:name="DeckList">
            <node oor:name="LocalWriterDeck" oor:op="replace">
                <prop oor:name="Title" oor:type="xs:string">
                    <value xml:lang="en-US">LocalWriter</value>
                </prop>
                <prop oor:name="Id" oor:type="xs:string">
                    <value>LocalWriterDeck</value>
                </prop>
                <prop oor:name="IconURL" oor:type="xs:string">
                    <value>vnd.sun.star.extension://org.extension.localwriter/assets/icon_16.png</value>
                </prop>
                <prop oor:name="ContextList">
                    <value oor:separator=";">com.sun.star.text.TextDocument, any, visible; com.sun.star.sheet.SpreadsheetDocument, any, visible; com.sun.star.drawing.DrawingDocument, any, visible; com.sun.star.presentation.PresentationDocument, any, visible;</value>
                </prop>
            </node>
        </node>
        <node oor:name="PanelList">
            <node oor:name="ChatPanel" oor:op="replace">
                <prop oor:name="Title" oor:type="xs:string">
                    <value xml:lang="en-US">Chat with Document</value>
                </prop>
                <prop oor:name="Id" oor:type="xs:string">
                    <value>ChatPanel</value>
                </prop>
                <prop oor:name="DeckId" oor:type="xs:string">
                    <value>LocalWriterDeck</value>
                </prop>
                <prop oor:name="ContextList">
                    <value oor:separator=";">com.sun.star.text.TextDocument, any, visible; com.sun.star.sheet.SpreadsheetDocument, any, visible; com.sun.star.drawing.DrawingDocument, any, visible; com.sun.star.presentation.PresentationDocument, any, visible;</value>
                </prop>
                <prop oor:name="ImplementationURL" oor:type="xs:string">
                    <value>private:resource/toolpanel/ChatPanelFactory/ChatPanel</value>
                </prop>
                <prop oor:name="OrderIndex" oor:type="xs:int">
                    <value>100</value>
                </prop>
                <prop oor:name="WantsCanvas" oor:type="xs:boolean">
                    <value>false</value>
                </prop>
                <prop oor:name="WantsAWT" oor:type="xs:boolean">
                    <value>true</value>
                </prop>
                <prop oor:name="DefaultMenuCommand">
                    <value></value>
                </prop>
            </node>
        </node>
    </node>
</oor:component-data>
